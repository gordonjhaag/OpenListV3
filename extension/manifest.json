{
  "name": "OpenList",
  "author": "Gordon Haag",
  "manifest_version": 3,
  "version": "0.1.0",
  "description": "Utilities to create and open lists of tabs. v3",
  "homepage_url": "https://github.com/gordonjhaag/OpenListV3",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_icon":  "icon/icon-48.png",
    "default_title": "Tab List",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon/icon-16.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  }
}
